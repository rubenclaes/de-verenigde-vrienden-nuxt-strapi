    "@nuxtjs/sitemap": "^2.1.0",
    "@stripe/stripe-js": "^1.5.0",
    "@vue/composition-api": "^0.3.4",
    "aos": "^2.3.4",
    "bootstrap-vue": "^2.10.1",
    "dotenv": "^8.2.0",
    "lazysizes": "^5.2.0",
    "nouislider": "^14.1.1",
    "nuxt": "^2.12.0",
    "nuxt-cookie-control": "^1.8.4",
    "nuxt-gmaps": "^1.1.8",
    "nuxt-payload-extractor": "^1.0.0",
    "nuxt-property-decorator": "^2.7.2",
    "sweetalert2": "^9.10.9",
    "uuid": "^7.0.2",
    "vee-validate": "^3.2.5",
    "vue-clamp": "^0.2.2",
    "vue-confetti": "^2.0.8",
    "vue-headroom": "^0.10.1",
    "vue-if-bot": "^1.2.0",
    "vue-observe-visibility": "^0.4.6",
    "vue-scrollto": "^2.17.1",
