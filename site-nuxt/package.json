{
  "name": "de-verenigde-vrienden-nuxt-strapi",
  "version": "2.1.0",
  "description": "A Nuxt.js fully satic site for Harmonie De Verenigde Vrienden",
  "author": "Ruben Claes",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "analyze": "nuxt-ts build --analyze",
    "generate": "nuxt-ts generate --devtools",
    "start": "nuxt-ts start",
    "tsc": "tsc --showConfig",
    "test:watch": "jest --watchAll",
    "testc": "tsc -p tsconfig.json",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project .",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "static": "npm run generate"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.3",
    "@nuxtjs/composition-api": "^0.16.4",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sentry": "^4.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@stripe/stripe-js": "^1.11.0",
    "aos": "^2.3.4",
    "bootstrap-vue": "^2.20.1",
    "lazysizes": "^5.2.2",
    "nouislider": "^14.6.3",
    "nuxt": "2.14.9",
    "nuxt-cookie-control": "^1.9.6",
    "nuxt-gmaps": "^1.2.10",
    "nuxt-property-decorator": "^2.8.8",
    "object-fit-images": "^3.2.4",
    "sweetalert2": "^9.17.2",
    "uuid": "^8.3.1",
    "vee-validate": "^3.4.5",
    "vue-backtotop": "^1.6.1",
    "vue-confetti": "^2.2.1",
    "vue-glide-js": "^1.3.14",
    "vue-headroom": "^0.10.1",
    "vue-if-bot": "^1.2.0",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "vue-observe-visibility": "^0.4.6",
    "vue-scrollto": "^2.20.0",
    "vue-toasted": "^1.1.28",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.3.0",
    "@nuxt/types": "^2.14.9",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/color-mode": "^2.0.2",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/jest": "^26.0.16",
    "@vue/test-utils": "^1.1.1",
    "babel-core": "6.26.3",
    "babel-jest": "^26.6.3",
    "fs-extra": "^9.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "lqip-loader": "^2.2.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "ts-jest": "^26.4.4",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "vue-jest": "^3.0.7",
    "webp-loader": "^0.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
