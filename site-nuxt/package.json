{
  "name": "de-verenigde-vrienden-nuxt-strapi",
  "version": "2.0.1",
  "description": "A Nuxt.js satic site with Strapi backend",
  "author": "Ruben Claes",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "analyze": "nuxt-ts build --analyze",
    "generate": "nuxt-ts generate --devtools",
    "start": "nuxt-ts start",
    "tsc": "tsc --showConfig",
    "test:watch": "jest --watchAll",
    "testc": "tsc -p tsconfig.json",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project .",
    "build:static": "nuxt-ts build --target static",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "static": "npm run build:static && npm run export"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/axios": "^5.12.1",
    "@nuxtjs/markdownit": "^1.2.10",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sentry": "^4.3.4",
    "@nuxtjs/sitemap": "^2.4.0",
    "@stripe/stripe-js": "^1.9.0",
    "@vue/composition-api": "^0.6.6",
    "aos": "^2.3.4",
    "bootstrap-vue": "^2.15.0",
    "lazysizes": "^5.2.2",
    "nouislider": "^14.1.1",
    "nuxt": "2.13.1",
    "nuxt-cookie-control": "^1.8.6",
    "nuxt-gmaps": "^1.2.5",
    "nuxt-property-decorator": "^2.7.2",
    "sweetalert2": "^9.17.1",
    "uuid": "^7.0.3",
    "vee-validate": "^3.2.5",
    "vue-backtotop": "^1.6.1",
    "vue-confetti": "^2.0.9",
    "vue-glide-js": "^1.3.14",
    "vue-headroom": "^0.10.1",
    "vue-if-bot": "^1.2.0",
    "vue-observe-visibility": "^0.4.6",
    "vue-scrollto": "^2.18.2",
    "vue-toasted": "^1.1.28",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.1.0",
    "@nuxt/types": "^2.14.1",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxtjs/pwa": "^2.6.0",
    "@types/jest": "^26.0.9",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "6.26.3",
    "babel-jest": "^26.3.0",
    "fs-extra": "^9.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "lqip-loader": "^2.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "ts-jest": "^24.3.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "vue-jest": "^3.0.6",
    "webp-loader": "^0.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
