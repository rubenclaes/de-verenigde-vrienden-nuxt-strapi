  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.0.1",
    "@nuxt/typescript-build": "^0.6.6",
    "@types/jest": "^24.0.19",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "lqip-loader": "^2.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "ts-jest": "^24.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "vue-jest": "^3.0.5",
    "webp-loader": "^0.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
