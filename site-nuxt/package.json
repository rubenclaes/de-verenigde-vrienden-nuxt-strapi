{
  "name": "de-verenigde-vrienden-nuxt-strapi",
  "version": "2.0.1",
  "description": "A Nuxt.js satic site with Strapi backend",
  "author": "Ruben Claes",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "analyze": "nuxt-ts build --analyze",
    "generate": "nuxt-ts generate --devtools",
    "start": "nuxt-ts start",
    "tsc": "tsc --showConfig",
    "test:watch": "jest --watchAll",
    "testc": "tsc -p tsconfig.json",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project ."
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.6",
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/markdownit": "^1.2.9",
