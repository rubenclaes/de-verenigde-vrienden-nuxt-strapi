    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/markdownit": "^1.2.6",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sentry": "^3.0.0",
    "@nuxtjs/sitemap": "^1.3.0",
    "aos": "^2.3.4",
    "bootstrap-vue": "^2.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "lodash": "^4.17.15",
    "nouislider": "^13.1.5",
    "nuxt": "^2.10.0",
    "nuxt-property-decorator": "^2.4.0",
    "strapi-sdk-javascript": "^0.3.3",
