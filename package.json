    "@nuxtjs/sentry": "^3.0.1",
    "@nuxtjs/sitemap": "^2.0.1",
    "aos": "^2.3.4",
    "bootstrap-vue": "^2.1.0",
    "dotenv": "^8.2.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
