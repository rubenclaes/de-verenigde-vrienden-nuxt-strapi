{
  "name": "de-verenigde-vrienden-nuxt-strapi",
  "version": "1.0.0",
  "description": "A Nuxt.js project with strapi",
  "author": "Ruben Claes",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "analyze": "nuxt-ts build --analyze",
